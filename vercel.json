{
  "version": 2,
  "builds": [
    {
      "src": "api/simple.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/simple.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/simple.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  }
}